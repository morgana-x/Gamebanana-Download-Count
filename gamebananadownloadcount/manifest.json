
{
    "manifest_version": 3,
    "name": "Gamebanana Download Count+",
    "version": "1.0",
    "description": "Adds the download count to multiple submission posts on game / feed pages",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://gamebanana.com/*"
            ]
        }
    ]
}
